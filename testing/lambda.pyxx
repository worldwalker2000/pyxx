import random;

numbers = [random.randint(1, 50) for _ in range(40)];

numbers = list(map([](x) { return x * x; }, numbers));
print(true);

numbers = list(map([](x) {
    x++;
    x *= x;
    return x;
}, numbers));
print(true);

numbers = list(map([](x)
{
    x++;
    x *= x;
    return x;
}, numbers));

print(true);
